#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/prakhartomar/Desktop/Prakhar/EasyAppsAI/Astro Resume/node_modules/.pnpm/firebase-functions@6.3.2_firebase-admin@13.1.0/node_modules/firebase-functions/lib/bin/node_modules:/Users/prakhartomar/Desktop/Prakhar/EasyAppsAI/Astro Resume/node_modules/.pnpm/firebase-functions@6.3.2_firebase-admin@13.1.0/node_modules/firebase-functions/lib/node_modules:/Users/prakhartomar/Desktop/Prakhar/EasyAppsAI/Astro Resume/node_modules/.pnpm/firebase-functions@6.3.2_firebase-admin@13.1.0/node_modules/firebase-functions/node_modules:/Users/prakhartomar/Desktop/Prakhar/EasyAppsAI/Astro Resume/node_modules/.pnpm/firebase-functions@6.3.2_firebase-admin@13.1.0/node_modules:/Users/prakhartomar/Desktop/Prakhar/EasyAppsAI/Astro Resume/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/prakhartomar/Desktop/Prakhar/EasyAppsAI/Astro Resume/node_modules/.pnpm/firebase-functions@6.3.2_firebase-admin@13.1.0/node_modules/firebase-functions/lib/bin/node_modules:/Users/prakhartomar/Desktop/Prakhar/EasyAppsAI/Astro Resume/node_modules/.pnpm/firebase-functions@6.3.2_firebase-admin@13.1.0/node_modules/firebase-functions/lib/node_modules:/Users/prakhartomar/Desktop/Prakhar/EasyAppsAI/Astro Resume/node_modules/.pnpm/firebase-functions@6.3.2_firebase-admin@13.1.0/node_modules/firebase-functions/node_modules:/Users/prakhartomar/Desktop/Prakhar/EasyAppsAI/Astro Resume/node_modules/.pnpm/firebase-functions@6.3.2_firebase-admin@13.1.0/node_modules:/Users/prakhartomar/Desktop/Prakhar/EasyAppsAI/Astro Resume/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../firebase-functions/lib/bin/firebase-functions.js" "$@"
else
  exec node  "$basedir/../firebase-functions/lib/bin/firebase-functions.js" "$@"
fi
